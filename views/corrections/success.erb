<div class="row" style="margin-top:12em;">
  <div class="large-offset-4 large-4 columns">
    <div class="panel callout radius">
      <strong>Success!</strong> Added a <%= corrections.total %> corrections to the database.
    </div>
    <table class="table">
      <thead>
        <tr colspan="4">
          <th>All Health Centers</th>
        </tr>
        <tr>
          <th>CPT</th>
          <th>Drug</th>
          <th>Count</th>
          <th>Date</th>
      </thead>
      <tbody>
        <% data['corrections'].each do |correction| %>
          <tr>
            <td><%= correction.cpt %></td>
            <td><%= correction.name %></td>
            <td><%= correction.count %></td>
            <td><%= correction.date %></td>
          </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>
